    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <customErrors>
        <name>customeError</name>
        <label>customeError</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <customErrorMessages>
            <errorMessage>Queue not found for the record.
{!$Flow.FaultMessage}</errorMessage>
            <fieldSelection>OwnerId</fieldSelection>
            <isFieldError>true</isFieldError>
        </customErrorMessages>
    </customErrors>
    <decisions>
        <name>DC01_Check_if_region_chaged</name>
        <label>DC01 ; Check if region chaged</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Outcome_1_of_DC01_Check_if_region_chaged</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record__Prior.Region__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Region__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Region__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>GR01_get_Queue_record</targetReference>
            </connector>
            <label>Outcome 1 of DC01 ; Check if region chaged</label>
        </rules>
    </decisions>
    <decisions>
        <name>DC01_Queue_Found</name>
        <label>DC01 ; Queue Found ??</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>UR02_update</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Queue   not found</defaultConnectorLabel>
        <rules>
            <name>Queue_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GR01_get_Queue_record.Name</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UR01_Update_lead_record</targetReference>
            </connector>
            <label>Queue found</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <interviewLabel>Lead - Create record - with queue {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Lead - Create record - with queue</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>GR01_get_Queue_record</name>
        <label>GR01 ; get Queue record</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>DC01_Queue_Found</targetReference>
        </connector>
        <faultConnector>
            <targetReference>customeError</targetReference>
        </faultConnector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Type</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Queue</stringValue>
            </value>
        </filters>
        <filters>
            <field>Name</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Region__c</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Group</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordUpdates>
        <name>UR01_Update_lead_record</name>
        <label>UR01: Update lead record</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
